
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
</head>
<body>
    <table class="table table-hover">
        <thead>
            <tr>
                <td>ID</td>
                <td>Name</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var dept in ViewData["Department"] as List<Department>)
            {
                <tr>

                    <td>@dept.Id</td>
                    <td>@dept.Name</td>
                </tr>
            }
        </tbody>
    </table>

</body>
</html>
